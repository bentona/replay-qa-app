= form_for @user do |f|
  - if @user.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user.errors.count, "error")} prohibited this user from being saved:"
      %ul
        - @user.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :timezone
    = f.text_field :timezone
  .field
    = f.label :locale
    = f.text_field :locale
  .field
    = f.label :locale_id
    = f.text_field :locale_id
  .field
    = f.label :organizationID
    = f.text_field :organizationID
  .field
    = f.label :role
    = f.text_field :role
  .field
    = f.label :customRoleId
    = f.text_field :customRoleId
  .field
    = f.label :phone
    = f.text_field :phone
  .field
    = f.label :verified
    = f.text_field :verified
  .actions
    = f.submit 'Save'
